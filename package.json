{
  "name": "sxflynn-portfolio",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && stylelint:fix",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint . --fix --cache",
    "stylelint": "stylelint '**/*.css' --cache",
    "stylelint:fix": "stylelint --fix '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "wrangler:listDeployments": "wrangler pages deployment list --project-name sxflynnportfolio",
    "wrangler:deployMain": "npm run build && wrangler pages deploy --branch main"
  },
  "dependencies": {
    "@mantine/code-highlight": "^7.16.2",
    "@mantine/core": "^7.16.2",
    "@mantine/hooks": "^7.16.2",
    "@tabler/icons-react": "^3.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "postcss": "^8.5.1",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "stylelint": "^16.14.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "wrangler": "^3.107.3"
  }
}
